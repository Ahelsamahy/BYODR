{
  # Type of throttle control - direct or pid.
"driver.cc.control.type": "pid",
"driver.cc.static.gear.step": 2,
"driver.cc.static.speed.max": 20,
"driver.cc.static.speed.min": 0.1,
"driver.cc.throttle.dead-zone": 0.25,
"driver.cc.throttle.scale.forwards": 0.9,
"driver.cc.throttle.scale.backwards": 0.6,
# Direct settings - not used on pid control.
"driver.throttle.direct.up.momentum": 0.025,
"driver.throttle.direct.down.momentum": 0.5,
# Pid settings - not used on direct control.
"driver.cc.throttle.pid.cap.feedback": 1.0,
"driver.cc.stop.pid_controller.p": 0.055,
"driver.cc.throttle.pid_controller.p": 0.005,
"driver.cc.throttle.pid_controller.i": 0.8,
"driver.cc.throttle.pid_controller.d": 0.0,
# Steering.
# A dead-zone collapses smaller values to zero and renormalizes the scale.
# The threshold is used to bin values.
"driver.console.steering.dead-zone": 0.0,
"driver.handler.steering.diff.threshold": 0.0,
# Dagger.
"driver.dnn.dagger.conv.dropout": 0.2,
# Autopilot.
"driver.dnn.obstacle.scale.min": 0.0,
"driver.dnn.obstacle.scale.max": 99999.05,
"driver.dnn.obstacle.scale.speed.multiplier": 0.0,
"driver.dnn.steer.corridor.scale.min": 0.0,
"driver.dnn.steer.corridor.scale.max": 0.02,
"driver.autopilot.filter.momentum.up": 0.2,
"driver.autopilot.filter.momentum.down": 0.4,
"driver.autopilot.filter.ceiling": 1.5,
# Navigation.
"navigation.point.similarity.threshold": 0.68,
# Online trainer.
"trainer.train.base_lr": 5e-6,
"trainer.train.min_lr": 1e-6,
"trainer.train.num_threads": 2,
"trainer.train.batch_size": 100,
"trainer.train.display_interval": 50,
"trainer.train.train_iterations": 1000
}